<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata minClientVersion="3.3.0">
        <id>Octopus.Dependencies.TerraformCLI</id>
        <version>0.11.2.1</version>
        <description>Terraform CLI Package</description>
        <authors>Matthew Casperson</authors>
        <contentFiles>
            <files include="**/terraform.exe" buildAction="None" copyToOutput="false" />
            <files include="**/plugins/windows_386/*.exe" buildAction="None" copyToOutput="false" />
        </contentFiles>
    </metadata>
    <files>
        <file src="terraform.exe" target="contentFiles/any/any" />
		<file src="plugins/windows_386/*.exe" target="contentFiles/any/any/plugins/windows_386" />
    </files>
</package>
