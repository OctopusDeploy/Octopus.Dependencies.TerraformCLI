<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata minClientVersion="3.3.0">
        <id>Octopus.Dependencies.TerraformCLI</id>
        <version>$version$</version>
        <description>
        Terraform CLI Package

        $terraformVersion$
        
        Plugins:
        
        $plugins$
        </description>
        <authors>Octopus Deploy</authors>
        <licenseUrl>https://github.com/OctopusDeploy/Octopus.Dependencies.TerraformCLI/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/OctopusDeploy/Octopus.Dependencies.TerraformCLI</projectUrl>
        <contentFiles>
            <files include="contentFiles/any/**" buildAction="None" copyToOutput="false" />
        </contentFiles>
    </metadata>
    <files>
        <!-- https://docs.microsoft.com/en-us/nuget/reference/nuspec#package-folder-structure -->
        <file src="terraform.exe" target="contentFiles/any/win" />
		<file src="plugins/windows_386/**" target="contentFiles/any/win" />
    </files>
</package>
